<?xml version="1.0" encoding="utf-8"?>
<form>
	<title>Landing</title>
	<sampledata></sampledata>
	
  <fields name="landing-style">
    <fieldset name="landing-content" label="LANDING_CONTENT_LABEL" description="LANDING_CONTENT_DESC">
      <field name="position" type="moduleposition"
                   description="LANDING_POSITION_DESC"
                   label="LANDING_POSITION_LABEL"
                   default=""
                   maxlength="50" />
      <field name="enable-leave-slide" type="radio" default="0" label="LEAVE_SLIDE_CONTROL_LABEL" description="LEAVE_SLIDE_CONTROL_DESC" >
        <option value="0" class="off">JNO</option>
        <option value="1" class="on">JYES</option>
      </field>
    </fieldset>
  </fields>
</form>